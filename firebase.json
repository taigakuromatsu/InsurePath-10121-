{
  "hosting": {
    "site": "kensyu10121-insurepath",
    "public": "dist/third-management-app/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
          "destination": "/index.html"
      }
